local LocalScript9 = Instance.new("LocalScript")
LocalScript9.Source = [[
local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local Workspace = game:GetService("Workspace")

local player = Players.LocalPlayer
local camera = Workspace.CurrentCamera

local gui = player:WaitForChild("PlayerGui"):WaitForChild("AdminPanel")
local frame = gui:WaitForChild("SpectationControls")
local spectateBtn = gui.Panel.ServerCommandsFrame.Spectation.Spectate
local prevBtn = frame:WaitForChild("Previous")
local nextBtn = frame:WaitForChild("Next")
local leaveBtn = frame:WaitForChild("Leave")
local freecamBtn = frame:FindFirstChild("FreecamToggle")

local spectating, freecam, mouseLocked = false, false, true
local targets = {}
local index = 0
local moveSpeed = 50
local rotation = Vector2.new()
local sensitivity = 0.5

task.wait(0.1)
local onMobile = UserInputService.TouchEnabled and not UserInputService.KeyboardEnabled
if freecamBtn then
	freecamBtn.Visible = onMobile
	if onMobile then freecamBtn.Text = "Enable Free Cam" end
end

local function refreshTargets()
	targets = {}
	for _, p in ipairs(Players:GetPlayers()) do
		if p ~= player and p.Character and p.Character:FindFirstChild("HumanoidRootPart") then
			targets[#targets+1] = p
		end
	end
	index = (#targets > 0) and math.clamp(index, 1, #targets) or 0
end

local function teleportTo(cframe)
	local root = player.Character and player.Character:FindFirstChild("HumanoidRootPart")
	if root then player.Character:SetPrimaryPartCFrame(cframe + Vector3.new(0,3,0)) end
end

local function teleportToSpectatePoint()
	local mm = Workspace:FindFirstChild("SpawnPoints") and Workspace.SpawnPoints:FindFirstChild("MMPOINT")
	if mm then teleportTo(mm.CFrame) end
end

local function teleportToRandomSpawn()
	local sp = Workspace:FindFirstChild("SpawnPoints")
	if sp then
		local pts = {}
		for _, c in ipairs(sp:GetChildren()) do
			if c:IsA("BasePart") and c.Name ~= "MMPOINT" then
				pts[#pts+1] = c
			end
		end
		local count = math.min(9, #pts)
		if count > 0 then teleportTo(pts[math.random(1,count)].CFrame) end
	end
end

local function enterSpectate()
	if index == 0 then return end
	local t = targets[index]
	if t and t.Character then
		camera.CameraType = Enum.CameraType.Follow
		camera.CameraSubject = t.Character:FindFirstChildOfClass("Humanoid")
	end
end

local function exitSpectate()
	camera.CameraType = Enum.CameraType.Custom
	if player.Character then
		camera.CameraSubject = player.Character:FindFirstChildOfClass("Humanoid")
	end
end

local function updateMouseLock()
	UserInputService.MouseBehavior = mouseLocked and Enum.MouseBehavior.LockCenter or Enum.MouseBehavior.Default
end

local function enableFreecam()
	freecam = true
	camera.CameraType = Enum.CameraType.Scriptable
	if freecamBtn then freecamBtn.Text = "Free Cam" end
	rotation = Vector2.new()
	mouseLocked = true
	updateMouseLock()
end

local function disableFreecam()
	freecam = false
	if freecamBtn then freecamBtn.Text = "Enable Free Cam" end
	UserInputService.MouseBehavior = Enum.MouseBehavior.Default
	enterSpectate()
end

UserInputService.InputChanged:Connect(function(input)
	if not freecam or not mouseLocked then return end
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		local d = input.Delta
		if typeof(d) == "Vector2" then
			rotation += Vector2.new(-d.X, -d.Y) * sensitivity
		end
	end
end)

RunService.RenderStepped:Connect(function(dt)
	if freecam then
		rotation = Vector2.new(rotation.X, math.clamp(rotation.Y, -85, 85))
		local yaw, pitch = math.rad(rotation.X), math.rad(rotation.Y)
		camera.CFrame = CFrame.new(camera.CFrame.Position) * CFrame.Angles(0, yaw, 0) * CFrame.Angles(pitch, 0, 0)

		local dir = Vector3.zero
		local look, right = camera.CFrame.LookVector, camera.CFrame.RightVector
		if UserInputService:IsKeyDown(Enum.KeyCode.W) then dir += look end
		if UserInputService:IsKeyDown(Enum.KeyCode.S) then dir -= look end
		if UserInputService:IsKeyDown(Enum.KeyCode.A) then dir -= right end
		if UserInputService:IsKeyDown(Enum.KeyCode.D) then dir += right end
		if UserInputService:IsKeyDown(Enum.KeyCode.E) then dir += Vector3.new(0,1,0) end
		if UserInputService:IsKeyDown(Enum.KeyCode.Q) then dir -= Vector3.new(0,1,0) end
		if dir.Magnitude > 0 then camera.CFrame += dir.Unit * moveSpeed * dt end
	end
end)

local function toggleSpectate()
	spectating = not spectating
	if spectating then
		teleportToSpectatePoint()
		refreshTargets()
		index = (#targets > 0) and math.random(1, math.min(9, #targets)) or 0
		enterSpectate()
		frame.Visible = true
		spectateBtn.Text = "Stop Spectating"
	else
		disableFreecam()
		exitSpectate()
		teleportToRandomSpawn()
		frame.Visible = false
		spectateBtn.Text = "Spectate"
	end
end

spectateBtn.MouseButton1Click:Connect(toggleSpectate)

prevBtn.MouseButton1Click:Connect(function()
	if spectating then
		index = (index == 1) and #targets or index - 1
		enterSpectate()
	end
end)

nextBtn.MouseButton1Click:Connect(function()
	if spectating then
		index = (index == #targets) and 1 or index + 1
		enterSpectate()
	end
end)

leaveBtn.MouseButton1Click:Connect(function()
	if spectating then toggleSpectate() end
end)

if freecamBtn then
	freecamBtn.MouseButton1Click:Connect(function()
		if spectating then
			if freecam then disableFreecam() else enableFreecam() end
		end
	end)
end

UserInputService.InputBegan:Connect(function(input, processed)
	if processed or not spectating then return end
	if input.KeyCode == Enum.KeyCode.Space then
		if freecam then disableFreecam() else enableFreecam() end
	elseif input.KeyCode == Enum.KeyCode.C and freecam then
		mouseLocked = not mouseLocked
		updateMouseLock()
	end
end)

Players.PlayerAdded:Connect(function()
	if spectating then refreshTargets() end
end)

Players.PlayerRemoving:Connect(function()
	if spectating then refreshTargets() end
end)

player.CharacterAdded:Connect(function(char)
	if not spectating then
		local h = char:WaitForChild("Humanoid", 10)
		if h then camera.CameraSubject = h end
	end
end)

LocalScript9.Parent = SpectationControls

