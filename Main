local LocalScript7 = Instance.new("LocalScript")
LocalScript7.Source = "local Players = game:GetService(\"Players\")\nlocal RunService = game:GetService(\"RunService\")\nlocal UserInputService = game:GetService(\"UserInputService\")\nlocal Workspace = game:GetService(\"Workspace\")\n\nlocal player = Players.LocalPlayer\nlocal camera = Workspace.CurrentCamera\n\n-- UI references\nlocal gui = player:WaitForChild(\"PlayerGui\"):WaitForChild(\"ModMenuUI\")\nlocal frame = gui:WaitForChild(\"SpectationControls\")\nlocal spectateBtn = gui:WaitForChild(\"Panel\"):WaitForChild(\"UniversalMenu\"):WaitForChild(\"Spectation\"):WaitForChild(\"Spectate\")\nlocal prevBtn = frame:WaitForChild(\"Previous\")\nlocal nextBtn = frame:WaitForChild(\"Next\")\nlocal leaveBtn = frame:WaitForChild(\"Leave\")\nlocal freecamBtn = frame:FindFirstChild(\"FreecamToggle\")\n\n-- State variables\nlocal spectating = false\nlocal freecam = false\nlocal mouseLocked = true\nlocal targets = {}\nlocal index = 0\nlocal moveSpeed = 50\nlocal rotation = Vector2.new()\nlocal sensitivity = 0.5\n\n-- Platform detection & Freecam button\ntask.wait(0.1)\nlocal onMobile = UserInputService.TouchEnabled and not UserInputService.KeyboardEnabled\nif freecamBtn then\n\tfreecamBtn.Visible = onMobile\n\tif onMobile then freecamBtn.Text = \"Enable Free Cam\" end\nend\n\n-- Helper: rebuild target list\nlocal function refreshTargets()\n\ttargets = {}\n\tfor _, p in ipairs(Players:GetPlayers()) do\n\t\tif p ~= player and p.Character and p.Character:FindFirstChild(\"HumanoidRootPart\") then\n\t\t\ttable.insert(targets, p)\n\t\tend\n\tend\n\tindex = (#targets > 0) and math.clamp(index, 1, #targets) or 0\nend\n\n-- Teleport helpers\nlocal function teleportToSpectatePoint()\n\tlocal mm = Workspace:FindFirstChild(\"SpawnPoints\") and Workspace.SpawnPoints:FindFirstChild(\"MMPOINT\")\n\tif mm and player.Character then\n\t\tlocal root = player.Character:FindFirstChild(\"HumanoidRootPart\")\n\t\tif root then\n\t\t\tplayer.Character:SetPrimaryPartCFrame(mm.CFrame + Vector3.new(0, 3, 0))\n\t\tend\n\tend\nend\n\nlocal function teleportToRandomSpawn()\n\tlocal sp = Workspace:FindFirstChild(\"SpawnPoints\")\n\tif sp and player.Character then\n\t\tlocal root = player.Character:FindFirstChild(\"HumanoidRootPart\")\n\t\tif root then\n\t\t\tlocal pts = {}\n\t\t\tfor _, c in ipairs(sp:GetChildren()) do\n\t\t\t\tif c:IsA(\"BasePart\") and c.Name ~= \"MMPOINT\" then\n\t\t\t\t\ttable.insert(pts, c)\n\t\t\t\tend\n\t\t\tend\n\t\t\tlocal count = math.min(9, #pts)\n\t\t\tif count > 0 then\n\t\t\t\tlocal choice = pts[math.random(1, count)]\n\t\t\t\tplayer.Character:SetPrimaryPartCFrame(choice.CFrame + Vector3.new(0, 3, 0))\n\t\t\tend\n\t\tend\n\tend\nend\n\n-- Camera mode handlers\nlocal function enterSpectate()\n\tif index == 0 then return end\n\tlocal t = targets[index]\n\tif t and t.Character then\n\t\tcamera.CameraType = Enum.CameraType.Follow\n\t\tcamera.CameraSubject = t.Character:FindFirstChildOfClass(\"Humanoid\")\n\tend\nend\n\nlocal function exitSpectate()\n\tcamera.CameraType = Enum.CameraType.Custom\n\tif player.Character then\n\t\tcamera.CameraSubject = player.Character:FindFirstChildOfClass(\"Humanoid\")\n\tend\nend\n\nlocal function updateMouseLock()\n\tUserInputService.MouseBehavior = mouseLocked and Enum.MouseBehavior.LockCenter or Enum.MouseBehavior.Default\nend\n\nlocal function enableFreecam()\n\tfreecam = true\n\tcamera.CameraType = Enum.CameraType.Scriptable\n\tif freecamBtn then freecamBtn.Text = \"Free Cam\" end\n\trotation = Vector2.new()\n\tmouseLocked = true\n\tupdateMouseLock()\nend\n\nlocal function disableFreecam()\n\tfreecam = false\n\tif freecamBtn then freecamBtn.Text = \"Enable Free Cam\" end\n\tUserInputService.MouseBehavior = Enum.MouseBehavior.Default\n\tenterSpectate()\nend\n\n-- Input handling (with inverted Y fixed)\nlocal function onInputChanged(input)\n\tif not freecam or not mouseLocked then return end\n\tif input.UserInputType == Enum.UserInputType.MouseMovement then\n\t\tlocal d = input.Delta\n\t\trotation = rotation + Vector2.new(-d.X, -d.Y) * sensitivity\n\telseif input.UserInputType == Enum.UserInputType.Touch then\n\t\tlocal d = input.Delta\n\t\tif typeof(d) == \"Vector2\" then\n\t\t\trotation = rotation + Vector2.new(-d.X, -d.Y) * sensitivity\n\t\tend\n\tend\nend\nUserInputService.InputChanged:Connect(onInputChanged)\n\n-- Freecam update on each frame\nRunService.RenderStepped:Connect(function(dt)\n\tif freecam then\n\t\t-- clamp pitch\n\t\trotation = Vector2.new(rotation.X, math.clamp(rotation.Y, -85, 85))\n\t\tlocal yaw = math.rad(rotation.X)\n\t\tlocal pitch = math.rad(rotation.Y)\n\t\tlocal camPos = camera.CFrame.Position\n\t\tcamera.CFrame = CFrame.new(camPos)\n\t\t\t* CFrame.Angles(0, yaw, 0)\n\t\t\t* CFrame.Angles(pitch, 0, 0)\n\n\t\t-- movement\n\t\tlocal look = camera.CFrame.LookVector\n\t\tlocal right = camera.CFrame.RightVector\n\t\tlocal dir = Vector3.zero\n\t\tif UserInputService:IsKeyDown(Enum.KeyCode.W) then dir += look end\n\t\tif UserInputService:IsKeyDown(Enum.KeyCode.S) then dir -= look end\n\t\tif UserInputService:IsKeyDown(Enum.KeyCode.A) then dir -= right end\n\t\tif UserInputService:IsKeyDown(Enum.KeyCode.D) then dir += right end\n\t\tif UserInputService:IsKeyDown(Enum.KeyCode.E) then dir += Vector3.new(0,1,0) end\n\t\tif UserInputService:IsKeyDown(Enum.KeyCode.Q) then dir -= Vector3.new(0,1,0) end\n\t\tif dir.Magnitude > 0 then\n\t\t\tcamera.CFrame += dir.Unit * moveSpeed * dt\n\t\tend\n\tend\nend)\n\n-- Spectate toggle logic\nlocal function toggleSpectate()\n\tspectating = not spectating\n\tif spectating then\n\t\tteleportToSpectatePoint()\n\t\trefreshTargets()\n\t\tindex = #targets > 0 and math.random(1, math.min(9, #targets)) or 0\n\t\tenterSpectate()\n\t\tframe.Visible = true\n\t\tspectateBtn.Text = \"Stop Spectating\"\n\telse\n\t\tdisableFreecam()\n\t\texitSpectate()\n\t\tteleportToRandomSpawn()\n\t\tframe.Visible = false\n\t\tspectateBtn.Text = \"Spectate\"\n\tend\nend\nspectateBtn.MouseButton1Click:Connect(toggleSpectate)\n\n-- Cycle targets\nprevBtn.MouseButton1Click:Connect(function()\n\tif spectating then\n\t\tindex = (index == 1) and #targets or index - 1\n\t\tenterSpectate()\n\tend\nend)\nnextBtn.MouseButton1Click:Connect(function()\n\tif spectating then\n\t\tindex = (index == #targets) and 1 or index + 1\n\t\tenterSpectate()\n\tend\nend)\nleaveBtn.MouseButton1Click:Connect(function()\n\tif spectating then\n\t\ttoggleSpectate()\n\tend\nend)\n\n-- Freecam button\nif freecamBtn then\n\tfreecamBtn.MouseButton1Click:Connect(function()\n\t\tif spectating then\n\t\t\tif freecam then disableFreecam() else enableFreecam() end\n\t\tend\n\tend)\nend\n\n-- Keyboard controls\nUserInputService.InputBegan:Connect(function(input, gameProcessed)\n\tif gameProcessed or not spectating then return end\n\tif input.KeyCode == Enum.KeyCode.Space then\n\t\tif freecam then disableFreecam() else enableFreecam() end\n\telseif input.KeyCode == Enum.KeyCode.C and freecam then\n\t\tmouseLocked = not mouseLocked\n\t\tupdateMouseLock()\n\tend\nend)\n\n-- Update targets dynamically\nPlayers.PlayerAdded:Connect(function() if spectating then refreshTargets() end end)\nPlayers.PlayerRemoving:Connect(function() if spectating then refreshTargets() end end)\n\n-- Reset camera on respawn\nplayer.CharacterAdded:Connect(function(char)\n\tif not spectating then\n\t\tlocal h = char:WaitForChild(\"Humanoid\", 10)\n\t\tif h then camera.CameraSubject = h end\n\tend\nend)\n"
LocalScript7.Parent = SpectationControls

